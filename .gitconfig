[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[user]
	name = rdgifford
	email = robbie.gifford@gmail.com
[color]
	ui = true
[push]
	default = matching
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[core]
	autocrlf = input
	safecrlf = true
	editor = /usr/bin/vim
[credential]
	helper = osxkeychain
